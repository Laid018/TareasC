<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	
	<!-- CONEXION BD -->
	<connectionStrings>
		<add name="conString" connectionString="Server=DESKTOP-QTDUMVH\\SQLEXPRESS;Database=Tarea;Integrated Security=True"/>
	</connectionStrings>


	<!-- Servicio web -->
	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="WSTareasSoap">
					<security mode="None">
						<transport clientCredentialType="None" proxyCredentialType="None" realm=""/>
					</security>
				</binding>
			</basicHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://localhost:56417/WSTareas.asmx" binding="basicHttpBinding"
			 bindingConfiguration="WSTareasSoap" contract="WSTareas.WSTareasSoap"
			 name="WSTareasSoap" />
		</client>
		<behaviors>
			<serviceBehaviors>
				<behavior name="AuthenticationServiceTypeBehaviors">
					<serviceMetadata httpsGetEnabled="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<serviceHostingEnvironment
		  aspNetCompatibilityEnabled="true"/>
	</system.serviceModel>
</configuration>